       RowBox[{"gyro", "=", 
        RowBox[{"GetGyro", "[", 
         RowBox[{"field", ",", "nuc"}], "]"}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"metNames", "=", 
        RowBox[{"{", 
         RowBox[{
         "\"\<PE\>\"", ",", "\"\<PC\>\"", ",", "\"\<Piex\>\"", ",", 
          "\"\<Piin\>\"", ",", "\"\<GPE\>\"", ",", "\"\<GPC\>\"", ",", 
          "\"\<PCr\>\"", ",", "\"\<ATP\>\"", ",", "\"\<NAD\>\"", ",", 
          "\"\<UDPG\>\""}], "}"}]}], ";", "\[IndentingNewLine]", 
