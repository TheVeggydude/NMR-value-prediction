          RowBox[{"SpectraPpmShift", "\[Rule]", "0"}], ",", 
          RowBox[{"SpectraFieldStrength", "\[Rule]", "field"}], ",", 
          RowBox[{"SpectraNucleus", "\[Rule]", "nuc"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"time", "=", 
        RowBox[{"GetTimeRange", "[", 
         RowBox[{
          RowBox[{"fidsI", "[", 
           RowBox[{"[", "1", "]"}], "]"}], ",", "dw"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"fidsI", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"TimeShiftFid", "[", 
           RowBox[{"#", ",", "time", ",", "lw"}], "]"}], "&"}], "/@", 
         "fidsI"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "actual", " ", "loop", " ", "for", " ", "dynamic", " ", 
         "simulation"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"Table", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"metAmps", "=", 
           RowBox[{"{", 
            RowBox[{"17", ",", "30", ",", "39", ",", 
             RowBox[{"piint", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "0", ",", "64", ",", 
             RowBox[{"pcrt", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "250", ",", "18", ",", 
             "20"}], "}"}]}], ";", "\[IndentingNewLine]", 
          "\[IndentingNewLine]", 
          RowBox[{"specs", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"ShiftSpectra", "[", 
              RowBox[{
               RowBox[{"ShiftedFourier", "[", "#", "]"}], ",", 
