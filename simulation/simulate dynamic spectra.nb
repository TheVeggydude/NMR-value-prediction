       RowBox[{
        RowBox[{"{", 
         RowBox[{"names", ",", "fidsI", ",", "specs", ",", "table"}], "}"}], 
        "=", 
        RowBox[{"GetSpectraBasisFunctions", "[", 
         RowBox[{"metNames", ",", "\[IndentingNewLine]", 
          RowBox[{"BasisSequence", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"\"\<PulseAcquire\>\"", ",", "0"}], "}"}]}], 
          RowBox[{"(*", 
